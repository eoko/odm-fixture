{
    "name": "eoko/odm/fixture",
    "description":"OAuth2 dynamoDB v.3 adapter for Oauth2 Server",
    "keywords":["oauth","oauth2","auth","dynamodb"],
    "type":"library",
    "license":"MIT",

    "minimum-stability": "dev",
    "prefer-stable": true,

    "authors":[
        {
            "name":"Romain DARY",
            "email": "romain.dary@eoko.fr",
            "homepage":"http://eoko.fr"
        }
    ],
    "homepage": "http://github.com/eoko/oauth2-adapter-dynamodb",
    "require":{
        "php": ">=5.5",
        "aws/aws-sdk-php": "~3.2",
        "doctrine/common": "^2.6@dev",
        "zendframework/zend-filter": "^2.5@dev"
    },
    "autoload": {
        "psr-4": {
            "Eoko\\ODM\\Fixture\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Eoko\\ODM\\Fixture\\Test\\": "test/"
        }
    },
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/eoko/oauth2-adapter-dynamodb"
        },
        {
            "type": "vcs",
            "url": "https://github.com/eoko/exponential-backoff"
        }
    ],
    "require-dev": {
        "satooshi/php-coveralls": "0.6.*",
        "fabpot/php-cs-fixer": "1.7.*",
        "phpunit/phpunit": "^5.0@dev",
        "mockery/mockery": "dev-master",
        "zendframework/zend-servicemanager": "^2.5@dev"
    }
}
